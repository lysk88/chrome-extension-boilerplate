{
    "name": "chrome-extension-boilerplate",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "a basic Chrome Extention boilerplate for easy an kickstarting",
    "homepage_url": "https://www.example.com", 
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }